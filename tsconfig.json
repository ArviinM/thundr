{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "types": ["react-native", "jest", "node"],
    "typeRoots": [
      "@types",
      "./node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@hooks": ["./src/hooks"],
      "@atoms/*": ["./src/components/atoms/*"],
      "@molecules/*": ["./src/components/molecules/*"],
      "@organisms/*": ["./src/components/organisms/*"],
      "types/*": ["./@types/*"]
    }
  }
}
